{
  "name": "@nilzona/prettier-config",
  "description": "Prettier config to share between projects",
  "version": "0.1.0",
  "license": "ISC",
  "main": "./prettier-config.json",
  "dependencies": {
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "3.2.2",
    "prettier-plugin-packagejson": "2.4.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": "git@github.com:nilzona/prettier-config.git",
  "scripts": {
    "format:check": "prettier --check '**' --ignore-unknown",
    "format:write": "prettier --write '**' --ignore-unknown"
  }
}
